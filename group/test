class Solution {
public:
    vector<int> shortestSeq(vector<int>& big, vector<int>& small) {
        vector<int> ans;
        unordered_map<int,int> m;
        int count=0,j=0;
        for(auto x:small){
            if(!m.count(x)) count++;
            m[x]++;
        } 
        for(int i=0;i<big.size();i++){
            m[big[i]]--;
            if(m[big[i]]==0) count--;
            while(!count){
                m[big[j]]++;
                if(m[big[j]]>0){
                    count++;
                    if(ans.empty()||ans[1]-ans[0]>i-j) ans={j,i};
                } 
                j++;
            }
        }
        return ans;
    }
};